<!-- app/views/projects/new.html.erb -->
<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>
    <h1>Create new project!</h1>
    <%= form_for @my_project do |f| %>
      <%= f.label :name %>
